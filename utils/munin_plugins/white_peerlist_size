#!/bin/bash

case $1 in
   config)
        cat <<'EOM'
graph_title white_peerlist_size
graph_vlabel white_peerlist_size
graph_category boolb
white_peerlist_size.label white_peerlist_size
EOM
        exit 0;;
esac

printf "white_peerlist_size.value "
connectivity_tool --ip=127.0.0.1 --rpc_port=37709 --timeout=1000 --rpc_get_daemon_info | grep white_peerlist_size | cut -d ' ' -f2
